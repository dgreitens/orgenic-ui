{
  "name": "@orgenic/orgenic-ui",
  "version": "0.5.0",
  "description": "ORGENIC UI",
  "author": {
    "name": "MAXIMAGO GmbH",
    "email": "dev@maximago.de"
  },
  "homepage": "https://orgenic.org/",
  "license": "MIT",
  "keywords": [
    "web components",
    "components",
    "custom elements",
    "user experience",
    "UX",
    "themes"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/orgenic/orgenic-ui.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "unpkg": "dist/orgenic-ui.js",
  "jsdelivr": "dist/orgenic-ui.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "build.watch": "stencil build --docs --watch",
    "start": "stencil build --dev --watch --serve --config stencil.run.config.ts",
    "test": "stencil test --spec --e2e",
    "test.spec": "stencil test --spec",
    "test.ci": "stencil test --spec --e2e --ci",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "check-licenseheader": "node ./.scripts/check-licenseheaders.js"
  },
  "pre-commit": [
    "check-licenseheader"
  ],
  "dependencies": {},
  "devDependencies": {
    "@stencil/core": "^2.5.2",
    "@stencil/sass": "^1.4.1",
    "@types/jest": "26.0.23",
    "@types/puppeteer": "5.4.3",
    "@types/tabulator-tables": "^4.9.1",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "acorn": "^8.2.4",
    "copyfiles": "^2.4.1",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "jest": "27.0.1",
    "jest-cli": "27.0.1",
    "moment": "^2.29.1",
    "node-sass": "^6.0.0",
    "pre-commit": "^1.2.2",
    "puppeteer": "9.1.1",
    "systemjs": "^6.9.0",
    "tabulator-tables": "^4.9.3",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  }
}
